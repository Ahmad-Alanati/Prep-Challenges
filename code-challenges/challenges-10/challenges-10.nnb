            "source": [
                "function migratoryBirds(arr) {\n    // Write your code here\n    let arr2=[0,0,0,0,0,0];\n    for(let bird of arr){\n        arr2[bird]++;\n    }\n    let max=1;\n    for(let bird=2;bird<arr2.length;bird++){\n        if(arr2[bird]>arr2[max]){\n            max = bird;\n        }\n    }\n    return max;\n}"
