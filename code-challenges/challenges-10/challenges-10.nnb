            "source": [
                "function birthdayCakeCandles(candles) {\n    // Write your code here\n    let count = 0;\n    let max=0;\n    for(let candle of candles){\n        if(candle>max){\n            max=candle;\n            count=0;\n        }\n        if(candle == max){\n            count++;\n        }\n    }\n    return count;\n}"
